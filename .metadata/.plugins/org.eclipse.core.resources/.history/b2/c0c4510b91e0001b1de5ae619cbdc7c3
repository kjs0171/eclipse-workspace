package practice;

public abstract class Car {
	
	protected String name;
	
	public abstract void start();
	public abstract void drive();
	public abstract void stop();
	public abstract void turnoff();
	
	public Car(String name) {
		this.name = name;
	}
	
	final public void run() {
	start();
	drive();
	stop();
	turnoff();
	
	}
}
