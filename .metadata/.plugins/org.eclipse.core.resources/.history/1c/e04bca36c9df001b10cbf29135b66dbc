package abstractex;

public class Coumputer {		// 추상 메소드 포함. 템플릿, 프레임워크 구현, 인터페이스 구현.
	public void display();		// 추상 메소드 : 선언부만 존재하고, 실행부가 없는 메소드
	public void typing();		// 추상 메소드
	
	//public void add(int a, int b) {}		// 일반 메소드
	public void turnOn() {
		System.out.println("전원을 켭니다.");
	}
	public void turnOff() {
		System.out.println("전원을 끕니다.");
	}
}
