package inheritance;

public class ColorPointEx {

	public static void main(String[] args) {
		
		Point p = new Point();				// Point 클래스의 객체새성자
		
		p.set(1, 2);
		p.showPoint();
		
		ColorPoint cp = new ColorPoint();	// ColorPoint  객체
		cp.set(3, 4);
		cp.setColor("red");
		cp.showColerPoint();
		
	}

}
